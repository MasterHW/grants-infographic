{
  "compilerOptions": {
      "lib": [
      "es2020",  "esnext"
    ],
    "module": "commonjs",
    "target": "es2020",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "custom.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "app/[chainId]/[roundId]/opengraph-image.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
